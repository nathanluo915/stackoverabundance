<div class="index-nav-bar"><span class="float-left" id="current-content-on-index">Most recent questions</span><span class="float-right">
<ul>
  <li><%= link_to "most recent", "/" %></li>
  <li><%= link_to "highest-voted", "/" %></li>
  <li><%= link_to "trending", "/" %></li>
</ul>
</span>
</div>
<div class="index-content">
  <% @questions.each do |q| %>
  <div class="index-content-item"><div class="index-content-left-side"><ul><li><%= q.get_votes_count %><br>votes</li><li><%= q.get_answers_count %><br>answers</li></ul>
  </div>
  <div class="index-content-right-side">
    <%= link_to q.title, question_path(q) %>
    <p>modified <%= q.get_modified_time %> <%= link_to q.user.username, user_path(q.user) %></p>
  </div>
</div>
  <% end %>
</div>
